
public class Ejercicio00 {

	
	
	public boolean checkDiag(int[][] matA) {
		
		
		//Dada una matriz de NxN dimensiones
		int n = matA.length;
		int total = 0;
		
		
		for(int i=0 ; i<n ;i++) {
			
			total += matA[i][i];
			if(i+1 < n) {
				if(matA[i+1][i+1] != total) {
					
				}
			}
		}
		return true;
	}
	
	
}

