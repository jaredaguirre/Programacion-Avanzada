
public class Main {

	
	public static void main(String[] args) {
		
		Circulo c1 = new Circulo(new Punto(0, 0), 1);
		Circulo c2 = new Circulo(new Punto(1, 1), 1.5);
		System.out.println(c1.intersectaCon(c2)); // true
		
		Circulo c3 = new Circulo(new Punto(0, 1), 2);
		Circulo c4 = new Circulo(new Punto(0, -2.4), 1);
		System.out.println(c3.intersectaCon(c4)); // true
	}
}
